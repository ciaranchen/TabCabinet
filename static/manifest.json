{
  "name": "TabCabinet",
  "description": "Yet another One Tab alternative",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "notifications",
    "idle",
    "alarms",
    "cookies",
    "webRequest"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "homepage_url": "https://github.com/ciaranchen/TabCabinet",
  "action": {
    "default_title": "Yet another One Tab alternative",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "default_locale": "zh_CN",
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}